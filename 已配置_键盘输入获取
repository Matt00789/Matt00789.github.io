    <span class="keyword">return</span> result</span><br><span class="line"></span><br><span class="line"><span class="function"><span class="keyword">def</span> <span class="title">bytes_to_bignum</span>(<span class="params">bs</span>):</span></span><br><span class="line">    result = <span class="number">0</span></span><br><span class="line">    exp    = <span class="number">0</span></span><br><span class="line">    <span class="keyword">for</span> b <span class="keyword">in</span> bs:</span><br><span class="line">        n = b % <span class="number">128</span></span><br><span class="line">        result += n &lt;&lt; exp</span><br><span class="line">        exp += <span class="number">7</span></span><br><span class="line">        <span class="keyword">if</span> b &amp; (<span class="number">1</span> &lt;&lt; <span class="number">7</span>) == <span class="number">0</span>:</span><br><span class="line">            <span class="keyword">break</span></span><br><span class="line">    <span class="keyword">return</span> result</span><br></pre></td></tr></table></figure>

